{
    "name": "fcl",
    "displayName": "Fermilab Hierarchical Configuration Language support",
    "publisher": "robertosoleti",
    "repository": "https://github.com/soleti/fcl_vscode",
    "description": "Fermilab Hierarchical Configuration Language",
    "version": "0.1.2",
    "icon": "fnal_logo.png",
    "engines": {
        "vscode": "^1.33.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "fcl",
            "aliases": ["FHiCL", "fcl"],
            "extensions": [".fcl"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "fcl",
            "scopeName": "source.fcl",
            "path": "./syntaxes/fcl.tmLanguage.json"
        }]
    }
}